{
  "name": "web",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/web/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/web",
        "index": "apps/web/src/index.html",
        "baseHref": "/",
        "outputHashing": "media",
        "main": "apps/web/src/main.tsx",
        "tsConfig": "apps/web/tsconfig.app.json",
        "assets": [
          "apps/web/src/favicon.png",
          "apps/web/src/assets",
          {
            "glob": "*.*",
            "input": "apps/web/src/assets/static/media",
            "output": "./media"
          },
          {
            "glob": "*.css",
            "input": "apps/web/src/assets/static",
            "output": "./"
          }
        ],
        "styles": [],
        "scripts": [],
        "isolatedConfig": true,
        "webpackConfig": "apps/web/webpack.config.js"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/web/src/environments/environment.ts",
              "with": "apps/web/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "build-dist": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/web",
        "index": "apps/web/src/index.html",
        "baseHref": "/",
        "outputHashing": "media",
        "main": "apps/web/src/main.tsx",
        "tsConfig": "apps/web/tsconfig.app.json",
        "assets": ["apps/web/src/favicon.png", "apps/web/src/assets",
        {
          "glob": "_headers",
          "input": "apps/web/src",
          "output": "./"
        },
        {
          "glob": "CNAME",
          "input": "apps/web/src",
          "output": "./"
        }],
        "styles": [],
        "scripts": [],
        "isolatedConfig": true,
        "webpackConfig": "apps/web/webpack.config.js"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/web/src/environments/environment.ts",
              "with": "apps/web/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "build-cordova": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "cordova/www",
        "index": "apps/web/src/index.html",
        "baseHref": "./",
        "outputHashing": "media",
        "main": "apps/web/src/main.tsx",
        "tsConfig": "apps/web/tsconfig.app.json",
        "assets": ["apps/web/src/favicon.png", "apps/web/src/assets"],
        "styles": [],
        "scripts": [],
        "isolatedConfig": true,
        "webpackConfig": "apps/web/webpack.config.js"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/web/src/environments/environment.ts",
              "with": "apps/web/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "build-test": {
      "command": "node tools/scripts/test.js",
      "args": "--ver=2002"
    },
    "build-themes": {
      "command": "webpack --config apps/web/webpack.theme.js --mode=production"
    },
    "build-scss": {
      "command": "node tools/scripts/build-scss.js"
    },
    "build-themes-local": {
      "command": "webpack --config apps/web/webpack.theme.js"
    },

    "build-server": {
      "command": "node tools/scripts/build-web.js --server=true",
      "dependsOn": ["build-dist", "build-scss", "build-themes"],
      "options": {
        "outputPath": "dist/apps/web"
      }
    },
    "clean-assets": {
      "command": "rm -rf apps/web/src/assets/static && mkdir apps/web/src/assets/static && mkdir apps/web/src/assets/static/media"
    },
    "dist": {
      "command": "node tools/scripts/build-pages.js && node tools/scripts/build-web.js && node tools/scripts/dist.js",
      "dependsOn": ["clean-assets", "build-dist", "build-scss", "build-themes"]
    },
    "dist:validate": {
      "command": "http-server ./dist/apps/web",
      "dependsOn": ["dist"]
    },
    "deploy-web": {
      "command": "npm version patch && rm -rf static-deploy/web/*  && cp -r dist/apps/web static-deploy/ && cd static-deploy/web && git add . && git commit -m \"update website\" && git push --force",
      "dependsOn": ["dist"]
    },
    "dist:local": {
      "command": "node tools/scripts/dist-local.js",
      "dependsOn": ["dist"]
    },

    "serve": {
      "executor": "@nx/webpack:dev-server",
      "dependsOn": ["build-themes-local"],
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "web:build",
        "proxyConfig": "apps/web/proxy.config.json",
        "hmr": true,
        "port": 4609
      },
      "configurations": {
        "development": {
          "buildTarget": "web:build:development"
        },
        "production": {
          "buildTarget": "web:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/web/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "web:dist"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/web/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
